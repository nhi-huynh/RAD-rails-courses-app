<p id="notice"><%= notice %></p>

<h1>All Coordinators</h1>

<% @coordinators.each do |coordinator| %>
  <div class="row">
    <div class="col">
      <image src=<%= coordinator.photo_url %> class="img-circle"></image>
    </div>
  </div>
  <div class="row">
    <div class="col">
        <p>Coordinator name: <%= coordinator.name %></p>
        <p></p>
    </div>
  </div>
<% end %>
